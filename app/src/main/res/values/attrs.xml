<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="sp" format="string"/>
    
    <declare-styleable name="AutoNewLineLayout">
        <attr name="horizontalSpace" format="dimension|reference"/>
        <attr name="vertivalSpace" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="SPCheckBox">
        <attr name="sp"/>
        <attr name="key" format="string"/>
        <attr name="value" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SPRadioGroup">
        <attr name="sp"/>
        <attr name="defCheck" format="integer"/>
    </declare-styleable>
    

    <declare-styleable name="CustomButton">
        <attr name="click" format="string"/>
    </declare-styleable>
    
        
    <declare-styleable name="ClearableEditText">
        <attr format="color|reference" name="clearIconDrawable"/>
        <attr format="boolean" name="clearIconDrawWhenFocused"/>
    </declare-styleable>
    
</resources>
